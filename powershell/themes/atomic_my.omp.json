{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#b2bec3",
          "foreground": "#222222",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": " {{.Icon}} ",
          "trailing_diamond": "\ue0b4",
          "type": "os"
        },
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "style": "diamond",
          "template": " \uf120 {{ .Name }} ",
          "trailing_diamond": "\ue0b4",
          "type": "shell"
        },
        {
          "background": "#ef5350",
          "foreground": "#FFFB38",
          "style": "diamond",
          "template": " \uf0ad ",
          "trailing_diamond": "\ue0b4",
          "type": "root"
        },
        {
          "background": "#FF9248",
          "foreground": "#2d3436",
          "properties": {
            "folder_icon": "\uf07b",
            "home_icon": "\ue617",
            "style": "full"
          },
          "style": "diamond",
          "template": " \uf07b {{ .Path }} ",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "#FFFB38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "foreground": "#011627",
          "properties": {
            "branch_icon": " \uf126 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "native_fallback": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        }
      ],
      "type": "prompt",
      "newline": true
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": "<#cc3a3a>\ue5fa</>",
            "yarn_icon": " <#348cba>\ue6a7</>"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#0e8ac8",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "background": "#906cff",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " <b>K</b> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "kotlin"
        },
        {
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "background": "#ffffff",
          "foreground": "#06aad5",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#f3f0ec",
          "foreground": "#925837",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "background": "#ffad55",
          "foreground": "#342311",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue6a9 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "zig"
        },
        {
          "background": "#e1e8e9",
          "foreground": "#055b9c",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "dart"
        },
        {
          "background": "#06A4CE",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue28e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "flutter"
        },
        {
          "background": "#ffffff",
          "foreground": "#ce092f",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "angular"
        },
        {
          "background": "#ffffff",
          "foreground": "#de1f84",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \u03b1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "aurelia"
        },
        {
          "background": "#1e293b",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "nx"
        },
        {
          "background": "#945bb3",
          "foreground": "#359a25",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " <#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#ffffff",
          "foreground": "#9c1006",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#ffffff",
          "foreground": "#5398c2",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background": "#565656",
          "foreground": "#faa029",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} ",
          "type": "aws"
        },
        {
          "background": "#316ce4",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "type": "kubectl"
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "style": "roundrock",
            "threshold": 500
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \ueba2 {{ .FormattedMs }}\u2800",
          "type": "executiontime"
        },
        {
          "background": "#00897b",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "foreground": "#d3d7cf",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " {{ if gt .Code 0 }}\uf071  {{ reason .Code }}{{ else }}\uf00c{{ end }} ",
          "type": "status"
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "foreground": "#262626",
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 ",
          "type": "battery"
        },
        {
          "background": "#d3d7cf",
          "foreground": "#000000",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#21c7c7",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#e0f8ff",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285\ueab6 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ if eq .PWD \"~\" }}~{{ else }}.../{{ base .PWD }}{{ end }}@{{ .HostName }} >_{{ .Shell }} #{{ .UserName }}",
  "version": 3
}